<script type="text/javascript">
var paymode =  $("#paymode").val();
paymode = paymode.toUpperCase(); //Added  for Gatewaylocking SIT fixes
var enableDisable =$("#enableDisable").val();
var secureurl;
secureurl = $('#secureurl').val();

function paymodeKey() {		//if(enableDisable == "E"){
	$.jCryption.getKeys(secureurl+"secure/EncryptionServlet?generateKeypair=true", function(receivedKeys) {keys = receivedKeys;
	   	if(keys != undefined || keys != "") {
	   		$(".overlay").css('display','none');
	   	}
	   	calcFees(paymode,this,this.id);  
	});
}

